    z-index: 805;
}

.at-indexes__menu-item {
    color: #6190e8;
    font-size: 24rpx;
    padding: 2rpx 10rpx;
    text-align: center;
}

.at-indexes__body {
    box-sizing: border-box;
    height: 100%;
    width: 100%;
}

.at-indexes__list,.at-indexes__list-title {
    box-sizing: border-box;
    overflow: hidden;
    width: 100%;
}

.at-indexes__list-title {
    background-color: #f7f7f7;
    color: #7f7f7f;
    font-size: 24rpx;
    padding: 10rpx 24rpx;
}

.at-list {
    background-color: #fff;
    position: relative;
}

.at-list::after {
    border-color: #d6e4ef;
    border-style: solid;
    border-width: 1px 0;
    bottom: -50%;
    box-sizing: border-box;
    content: "";
    left: -50%;
    pointer-events: none;
    position: absolute;
    right: -50%;
    top: -50%;
    transform: scale(.5);
    transform-origin: center;
}

.at-list__item-container {
    -ms-flex-align: center;
    -webkit-box-align: center;
    align-items: center;
    display: -ms-flexbox;
    display: flex;
}

.at-list__item-container .item-icon {
    margin-right: 24rpx;
}

.at-list__item {
    box-sizing: border-box;
    color: #333;
    font-size: 32rpx;
    line-height: 1.5;
    padding: 24rpx;
    position: relative;
    transition: background-color .3s;
}

.at-list__item:active {
    background-color: #f0f0f0;
}

.at-list__item::after {
    border-bottom: 1px solid #d6e4ef;
    bottom: 0;
    box-sizing: border-box;
    content: "";
    left: 24rpx;
    pointer-events: none;
    position: absolute;
    right: 0;
    top: auto;
    transform: scaleY(.5);
    transform-origin: center;
}

.at-list__item--thumb .item-thumb {
    height: 56rpx;
    margin-right: 24rpx;
    width: 56rpx;
}

.at-list__item--thumb .item-thumb__info {
    height: 100%;
    width: 100%;
}

.at-list__item--disabled .at-list__item-container {
    opacity: .3;
}

.at-list__item--disabled:active {
    background-color: initial!important;
}

.at-list__item--no-border::after {
    border: none;
    content: normal;
}

.at-list__item .item-content__info-title {
    color: inherit;
    font-size: inherit;
    line-height: 1.5;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.at-list__item .item-content__info-note {
    color: #999;
    font-size: 28rpx;
    line-height: 1.5;
}

.at-list__item .item-extra {
