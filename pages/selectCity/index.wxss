}

.at-list__item .item-extra__icon,.at-list__item .item-extra__image,.at-list__item .item-extra__info,.at-list__item .item-extra__switch {
    display: inline-block;
}

.at-list__item .item-extra__info {
    box-sizing: border-box;
    color: #999;
    font-size: 32rpx;
    max-width: 100%;
    overflow: hidden;
    padding-right: 30rpx;
    text-overflow: ellipsis;
    vertical-align: middle;
    white-space: nowrap;
}

.at-list__item .item-extra__icon {
    position: absolute;
    right: -12rpx;
    top: 50%;
    transform: translateY(-50%);
}

.at-list__item .item-extra__icon-arrow {
    color: #ccc;
    font-size: 48rpx;
}

.at-list__item .item-extra__switch {
    vertical-align: middle;
}

.at-list__item .item-extra__image {
    height: 56rpx;
    margin-right: 30rpx;
    vertical-align: middle;
    width: 56rpx;
}

.at-list__item .item-extra__image-info,.at-list__item .item-extra__image__info {
    height: 100%;
    width: 100%;
}

.at-list__item-content {
    -webkit-box-flex: 1;
    flex: 1;
    margin-right: 40rpx;
    overflow: hidden;
}

.at-list__item-extra {
    max-width: 235rpx;
    text-align: right;
}

.at-list--no-border::after,.at-list--no-border::before {
    border: none;
    content: normal;
}

